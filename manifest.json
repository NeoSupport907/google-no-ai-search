{
  "manifest_version": 3,
  "name": "No AI Search",
  "version": "6.7",
  "description": "Automatically appends a suffix to search queries on Google and custom domains.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}